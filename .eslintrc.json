{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
      "indent": ["error", 4],
      "no-use-before-define": 0,
      "key-spacing": 0,
      "react/jsx-indent": ["error", 4],
      "react/prefer-stateless-function": 0,
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "react/jsx-indent-props": [2, 4]
    },
    "parser": "babel-eslint"
}
